{
  "short_name": "Long",
  "name": "Only Long",
  "author": "Miras Shaltayev",
  "description": "Auto-correct English contractions in academic papers!",
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "version": "1.0.1",
  "manifest_version": 3,
  "action": {
    "default_icon": {
      "16": "./icons/icon16.ico",
      "48": "./icons/icon48.ico",
      "128": "./icons/icon128.ico"
    },
    "default_title": "Only Long",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "./icons/icon16.ico",
    "48": "./icons/icon48.ico",
    "128": "./icons/icon128.ico"
  },
  "background": {
    "service_worker": "background.js"
  }
}